{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": ["webpack-env"],
    // "alwaysStrict": true,
    // "noImplicitUseStrict": true,
    "noStrictGenericChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true

  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
